<h1><%=@doctor.name%></h1>

<ul>
<%@doctor.appointments.each do |a|%>
  <li><%=link_to(a.patient.name, patient_path(a.patient))%> - <%=a.appointment_datetime.strftime('%B %d, %Y at %H:%M')%></li>
<%end%>
</ul>

